<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    app:startDestination="@id/mainAuthF">

    <fragment
        android:id="@+id/getterAuthF"
        android:name="com.buyhelp.nofoodsharingproject.presentation.fragments.auth.GetterAuthFragment"
        android:label="fragment_getter_auth"
        tools:layout="@layout/fragment_getter_auth" >
        <action
            android:id="@+id/action_getterAuthF_to_mainAuthF"
            app:destination="@id/mainAuthF"
            app:enterAnim="@anim/nav_enter"
            app:exitAnim="@anim/nav_exit"
            app:popEnterAnim="@anim/nav_pop_enter"
            app:popExitAnim="@anim/nav_pop_exit"/>
    </fragment>
    <fragment
        android:id="@+id/setterAuthF"
        android:name="com.buyhelp.nofoodsharingproject.presentation.fragments.auth.SetterAuthFragment"
        android:label="fragment_setter_auth"
        tools:layout="@layout/fragment_setter_auth">
        <action
            android:id="@+id/action_setterAuthF_to_mainAuthF"
            app:destination="@id/mainAuthF"
            app:enterAnim="@anim/nav_enter"
            app:exitAnim="@anim/nav_exit"
            app:popEnterAnim="@anim/nav_pop_enter"
            app:popExitAnim="@anim/nav_pop_exit"
            />
        <action
            android:id="@+id/action_setterAuthF_to_setterLoginAuthF"
            app:destination="@id/setterLoginAuthF"
            app:enterAnim="@anim/nav_enter"
            app:exitAnim="@anim/nav_exit"
            app:popEnterAnim="@anim/nav_pop_enter"
            app:popExitAnim="@anim/nav_pop_exit"
            />
    </fragment>
    <fragment
        android:id="@+id/mainAuthF"
        android:name="com.buyhelp.nofoodsharingproject.presentation.fragments.auth.MainAuthFragment"
        android:label="fragment_main_auth"
        tools:layout="@layout/fragment_main_auth" >
        <action
            android:id="@+id/action_mainAuthF_to_setterAuthF"
            app:destination="@id/setterAuthF"
            app:enterAnim="@anim/nav_enter"
            app:exitAnim="@anim/nav_exit"
            app:popEnterAnim="@anim/nav_pop_enter"
            app:popExitAnim="@anim/nav_pop_exit"
            />
        <action
            android:id="@+id/action_mainAuthF_to_getterAuthF"
            app:destination="@id/getterAuthF"
            app:enterAnim="@anim/nav_enter"
            app:exitAnim="@anim/nav_exit"
            app:popEnterAnim="@anim/nav_pop_enter"
            app:popExitAnim="@anim/nav_pop_exit"
            />
    </fragment>
    <fragment
        android:id="@+id/setterLoginAuthF"
        android:name="com.buyhelp.nofoodsharingproject.presentation.fragments.auth.SetterLoginAuthFragment"
        android:label="fragment_setter_login_auth"
        tools:layout="@layout/fragment_setter_login_auth" >
        <action
            android:id="@+id/action_setterLoginAuthF_to_setterAuthF"
            app:destination="@id/setterAuthF"
            app:enterAnim="@anim/nav_enter"
            app:exitAnim="@anim/nav_exit"
            app:popEnterAnim="@anim/nav_pop_enter"
            app:popExitAnim="@anim/nav_pop_exit"
            />
    </fragment>
</navigation>